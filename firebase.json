{
  "hosting": {
    "public": "www",
    "ignore": [
      ".firebaserc",
      "database.rules.json",
      "firebase.json",
      "package.json",
      "**/.*",
      "**/node_modules/**",
      "**/src/**"
    ],
    "redirects": [
      {
        "source": "/styles.*.css",
        "destination": "/styles.css",
        "type": 301
      },
      {
        "source": "/tutorial/dynamic-response-with-templating",
        "destination": "https://github.com/255kb/mockoon/wiki/%7B%7B-%7D%7D-templating",
        "type": 302
      }
    ]
  }
}
