{
  "hosting": {
    "public": "www",
    "ignore": [
      ".firebaserc",
      "database.rules.json",
      "firebase.json",
      "package.json",
      "**/.*",
      "**/node_modules/**",
      "**/src/**"
    ],
    "redirects": [
      {
        "source": "/styles.*.css",
        "destination": "/styles.css",
        "type": 301
      }
    ]
  }
}
